{
  "name": "krowinski/laravel-tinyid",
  "description": "A TinyID bridge for Laravel",
  "keywords": [
    "laravel",
    "id",
    "obfuscate",
    "shortId",
    "ids",
    "php"
  ],
  "type": "library",
  "require": {
    "php": ">=7.1",
    "illuminate/contracts": "5.8.*",
    "illuminate/support": "5.8.*",
    "krowinski/tinyid": "^1.1",
    "graham-campbell/manager": "^4.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Kacper Rowi≈Ñski",
      "email": "kacper.rowinski@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "LaravelTinyID\\": "src/LaravelTinyID"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelTinyID\\TinyIDServiceProvider"
      ],
      "aliases": {
        "TinyID": "LaravelTinyID\\Facades\\TinyID"
      }
    }
  },
  "minimum-stability": "stable"
}
